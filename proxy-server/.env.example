# Server Configuration
PORT=3001
NODE_ENV=development

# EFI Bank Configuration
EFI_CLIENT_ID=your_client_id_here
EFI_CLIENT_SECRET=your_client_secret_here
EFI_PIX_KEY=your_pix_key_here
EFI_SANDBOX=true

# mTLS Certificate paths (for production)
EFI_CERTIFICATE_PATH=/path/to/certificate.crt
EFI_KEY_PATH=/path/to/private.key
EFI_CA_PATH=/path/to/ca.crt

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Optional
PROXY_BASE_URL=https://your-proxy-domain.com